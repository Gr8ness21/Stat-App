<h1>Players</h1>
<ul>
{{#each listOfPlayers}}
<li>
    <a href="/players/{{_id}}">
    {{name}}
    </a>
</li>
{{/each}}
</ul>

<form method="POST" action="/players">
    <label for="name">Name</label>
    <input type="text" name="name" value=""/>
    <input type="submit" value="Create New Player" />
</form>